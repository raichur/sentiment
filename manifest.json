{
  "name": "Sentiment",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "content_scripts":
[
    {
      "css": [ "style.css" ],
      "matches": ["https://*/*", "http://*/*"],
      "js": ["app.js"]
    }
],

  "browser_action": {
    "default_title": "Sentiment"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}
